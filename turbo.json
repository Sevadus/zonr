{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build:worker": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".open-next/**"],
      "outputLogs": "new-only"
    },
    "deploy:worker": {
      "dependsOn": ["build:worker"],
      "outputs": ["dist/**", ".open-next/**"],
      "outputLogs": "new-only"
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "outputLogs": "new-only"
    },
    "dev": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["build"]
    }
  }
}
